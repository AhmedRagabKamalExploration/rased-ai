{
  "name": "@rased-ai/web-sdk",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/web-sdk.umd.cjs",
  "module": "./dist/web-sdk.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/web-sdk.js",
      "require": "./dist/web-sdk.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^25.0.1",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "sdk",
    "web",
    "analytics",
    "tracking"
  ],
  "repository": {
    "type": "git",
    "url": "your-repo-url"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
